{
  "name": "notion",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@notionhq/client": "^0.4.9",
    "dayjs": "^1.10.7",
    "dotenv": "^10.0.0",
    "node-schedule": "^2.1.0"
  },
  "scripts": {
    "lint": "eslint . --ext .ts",
    "start:dev": "nodemon",
    "bash-deploy:dev": "npx tsc && docker build . -t maxime/notion && docker run maxime/notion",
    "bash-deploy": "npx tsc && heroku container:push web -a notionapi-maxime && heroku container:release web -a notionapi-maxime",
    "docker-start": "sudo service docker start",
    "logs": "heroku logs -n 50 -a notionapi-maxime",
    "start": "node build/index",
    "build": "yarn tsc"
  },
  "devDependencies": {
    "@types/node": "^16.11.10",
    "@types/node-schedule": "^1.3.2",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.3.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  }
}